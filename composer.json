{
    "name": "tbachert/sync",
    "description": "Synchronization primitives",
    "keywords": ["async", "lock", "synchronization"],
    "license": "Apache-2.0",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "revolt/event-loop": "^1.0"
    },
    "require-dev": {
        "amphp/amp": "^3.0",
        "amphp/phpunit-util": "^3.0",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Nevay\\Sync\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nevay\\Sync\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
